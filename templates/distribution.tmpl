<!doctype html>
<html>
{{ template "header" . }}
<body>
{{ template "navbar" . }}

<div class="container-fluid">
    <div class="row">
        <div class="col-md">
            <table class="table table-bordered table-sm">
                <thead>
                <tr>
                    <th>â„–</th>
                    <th>x</th>
                    <th>n</th>
                    <th>p</th>
                    <th>F<sub>N</sub>(x)</th>
                </tr>
                </thead>
                <tbody>
                {{range $index, $variant := .Variants}}
                    <tr>
                        <td>{{ add $index 1 }}</td>
                        <td>{{ $variant.X }}</td>
                        <td>{{ $variant.N }}</td>
                        <td>{{ printf "%.6f" $variant.P }}</td>
                        <td>{{ printf "%.6f" $variant.F }}</td>
                    </tr>
                {{end}}
                </tbody>
            </table>
        </div>

        <div class="col-md-auto order-first order-md-last">
            <div class="d-grid gap-2">
                <img class="img-fluid mx-auto d-block border" src="data:image/svg+xml;base64,{{.Image}}">
            </div>
        </div>
    </div>
</div>

{{template "footer" .}}
</body>
</html>